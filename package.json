    "styled-system": "^1.0.1",
    "unfetch": "^3.0.0"
  },
  "devDependencies": {
    "gatsby-plugin-styled-components": "^1.0.5",
    "gh-pages": "^0.12.0",
    "prettier": "^1.6.1",
    "surge": "^0.19.0"
  },
  "homepage": "https://github.com/noahg/gatsby-starter-blog-no-styles#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahg/gatsby-starter-blog-no-styles.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "develop": "gatsby develop",
    "remove-maps": "find ./public -name *.map | xargs rm",
    "build": "gatsby build && npm run remove-maps && echo jaredhill.co > public/CNAME",
    "deploy": "surge public"
  }
